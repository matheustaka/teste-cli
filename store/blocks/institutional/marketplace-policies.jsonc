  "flex-layout.col#content-marketplace-policies": {
    "children": [
      "rich-text#marketplace-policies--title",
      "rich-text#marketplace-policies--marketplace",
      "rich-text#marketplace-policies--frete",
      "rich-text#marketplace-policies--formas-pagamento",
      "rich-text#marketplace-policies--cupons-desconto",
      "rich-text#marketplace-policies--sac",
      "rich-text#marketplace-policies--seja-parceiro"
    ],
    "props": {
      "blockClass": "textColumn",
      "preventVerticalStretch": true
    }
