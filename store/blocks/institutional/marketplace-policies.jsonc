{
  "store.custom#marketplace-policies": {
    "blocks": [
      "responsive-layout.desktop#marketplace-policies",
      "responsive-layout.tablet#marketplace-policies",
      "responsive-layout.phone#marketplace-policies"
    ]
  },

  "responsive-layout.desktop#marketplace-policies": {
    "children": ["breadcrumb", "flex-layout.row#marketplace-policies-desktop"]
  },

  "responsive-layout.tablet#marketplace-policies": {
    "children": [
      "breadcrumb",
      "flex-layout.row#marketplace-policies-menu-mobile",
      "flex-layout.row#marketplace-policies-mobile"
    ]
  },

  "responsive-layout.phone#marketplace-policies": {
    "children": [
      "breadcrumb",
      "flex-layout.row#marketplace-policies-menu-mobile",
      "flex-layout.row#marketplace-policies-mobile"
    ]
  },

  "flex-layout.row#marketplace-policies-desktop": {
    "children": [
      "flex-layout.col#institutional-sidebar-desktop",
