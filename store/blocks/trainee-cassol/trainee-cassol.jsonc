        },
        "children" : [
            "image#step2Image",
            "rich-text#step2Text"
        ]
    },
    "image#step2Image" : {
        "props" : {
            "src" : "https://cassol.vteximg.com.br/arquivos/step3.jpg",
            "maxHeight" : 55,
            "maxWidth" : 55
        }
    },
    "rich-text#step2Text": {
        "props" : {
            "text": "#### Inscrições e realização dos testes <br> 15/03 a 15/04",
            "blockClass": "stepsText"
        }
    },
    "flex-layout.col#step3" : {
        "props" : {
            "horizontalAlign" : "center"
        },
        "children" : [
            "image#step3Image",
            "rich-text#step3Text"
        ]
    },
    "image#step3Image" : {
        "props" : {
            "src" : "https://cassol.vteximg.com.br/arquivos/step4.jpg",
            "maxHeight" : 55,
            "maxWidth" : 55
        }
    },
    "rich-text#step3Text": {
        "props" : {
            "text": "#### Inscrições e realização dos testes <br> 15/03 a 15/04",
            "blockClass": "stepsText"
        }
    },
    "flex-layout.col#step4" : {
        "props" : {
            "horizontalAlign" : "center"
        },
        "children" : [
            "image#step4Image",
            "rich-text#step4Text"
        ]
    },
    "image#step4Image" : {
        "props" : {
            "src" : "https://cassol.vteximg.com.br/arquivos/step5.jpg",
            "maxHeight" : 55,
            "maxWidth" : 55
        }
    },
    "rich-text#step4Text": {
        "props" : {
            "text": "#### Inscrições e realização dos testes <br> 15/03 a 15/04",
            "blockClass": "stepsText"
        }
    },
    "flex-layout.col#step5" : {
        "props" : {
