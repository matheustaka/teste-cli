        },
        "children" : [
            "image#step5Image",
            "rich-text#step5Text"
        ]
    },
    "image#step5Image" : {
        "props" : {
            "src" : "https://cassol.vteximg.com.br/arquivos/step1.jpg",
            "maxHeight" : 55,
            "maxWidth" : 55
        }
    },
    "rich-text#step5Text": {
        "props" : {
            "text": "#### Inscrições e realização dos testes <br> 15/03 a 15/04",
            "blockClass": "stepsText"
        }
    },

    // Development
    "flex-layout.row#development" : {
        "props" : {
            "paddingTop" :8,
            "paddingBottom" : 8,
            "blockClass" : ["developmentSection", "traineeMainPadding"]
        },
        "children" : [
            "flex-layout.col#developmentImage",
            "flex-layout.col#developmentTexts"
        ]
    },
    "flex-layout.col#developmentImage" : {
        "props" : {
            "horizontalAlign" : "center"
        },
        "children" :["image#developmentImage"]
    },
    "image#developmentImage" : {
        "props" : {
            "src" : ""
        }
    },
    "flex-layout.col#developmentTexts" : {
        "children" : [
            "flex-layout.row#developmentTitle",
            "flex-layout.row#developmentDescription"
        ]
    },
    "flex-layout.row#developmentTitle" : {
        "children": ["rich-text#devTitle"]
    },
    "rich-text#devTitle" : {
        "props" : {
            "text":"# Etapas de desenvolvimento"
        }
    },

    "flex-layout.row#developmentDescription": {
        "children" : ["rich-text#devDescription"]
    },
    "rich-text#devDescription" : {
        "props" : {
            "text" : "Em 2020, a Cassol foi reconhecida pelo prêmio Top of Mind pelas soluções digitais criadas para nos aproximar de nossos clientes, uma estratégia fundamental para o sucesso dos nossos canais de vendas e para manter a marca relevante para os consumidores durante o período de isolamento social."
        }
    },

    // expectation
    "flex-layout.row#expectation" : {
        "props" : {
            "horizontalAlign" : "middle",
            "paddingBottom" : 10,
            "paddingTop" : 10,
            "blockClass" : "expectationSection"
        },

        "children" : [
            "flex-layout.col#expectationText"
        ]
    },
    "flex-layout.col#expectationText" : {
        "props" : {
            "horizontalAlign" : "center"
        },
        "children" : ["rich-text#expectationTitle", "rich-text#expectationText"]
    },
    "rich-text#expectationTitle" : {
        "props" :{
            "text" : "# O que você pode esperar",
            "blockClass" : "stepsTitle",
            "textAlignment" : "center"
        }
    },
    "rich-text#expectationText" : {
        "props" :{
            "text" : "Visão sistêmica dos processos da empresa, desenvolvimento de conhecimentos ligados ao negócio da Cassol e à área de atuação do Trainee e acompanhamento durante todo o processo.",
            "blockClass" : "expectationText",
            "textAlignment" : "center"
        }
    },
    
    // Tablet
    "responsive-layout.tablet#traineeCassolTablet": {
        "children": [
            "list-context.image-list#traineeMainBanner",
            "flex-layout.row#traineeBrand",
            "flex-layout.row#traineeFeatures",
            "flex-layout.row#traineeSteps"
        ]
    },
    // Mobile
    "responsive-layout.phone#traineeCassolPhone": {
        "children": [
            "list-context.image-list#traineeMainBanner",
            "flex-layout.row#traineeBrand",
            "flex-layout.row#traineeFeatures",
            "flex-layout.row#traineeSteps"
        ]
    }
}