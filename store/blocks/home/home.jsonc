        "tablet": 2,
        "phone": 1
      },
      "infinite": false,
      "showNavigationArrows": "desktopOnly",
      "blockClass": "mini-banners"
    }
  },

  "rich-text#shelf-03": {
    "props": {
      "text": "## Última Oportunidade",
      "blockClass": "shelfTitle"
    }
  },
  "flex-layout.row#shelf-03": {
    "props": {
      "blockClass": "shelf-container"
    },
    "children": ["list-context.product-list#shelf-03"]
  },

  "list-context.product-list#shelf-03": {
    "blocks": ["product-summary.shelf"],
    "children": ["slider-layout#shelf-03"],
    "props": {
      "orderBy": "OrderByTopSaleDESC"
    }
  },

  "slider-layout#shelf-03": {
    "props": {
      "itemsPerPage": {
        "desktop": 4,
        "tablet": 3,
        "phone": 1
      },
      "infinite": true,
      "fullWidth": false,
      "blockClass": "shelf"
    }
  },

  "info-card#home": {
    "props": {
      "id": "info-card-home",
      "isFullModeStyle": false,
      "textPosition": "left",
      "imageUrl": "https://storecomponents.vteximg.com.br/arquivos/banner-infocard2.png",
      "headline": "Clearance Sale",
      "callToActionText": "DISCOVER",
      "callToActionUrl": "/sale/d",
      "blockClass": "info-card-home",
      "textAlignment": "center"
    }
  },

