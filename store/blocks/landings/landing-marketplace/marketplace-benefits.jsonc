    "props": {
      "text": "**Parceria** com os principais integradores do mercado"
    }
  },
  "image#mktplaceTornado": {
    "props": {
      "src": "assets/cassol-marketplace/tornado.png"
    }
  },
  "flex-layout.row#benefitsLeft-2": {
    "props": {
      "preventHorizontalStretch": true,
      "preventVerticalStretch": true,
      "blockClass": [
        "benefitsCardContainer"
      ]
