{
  "flex-layout.row#mktplaceBenefits-3": {
    "props": {
      "fullWidth": true,
      "horizontalAlign": "center",
      "colSizing": "auto",
      "blockClass": [
        "benefitsItemsContainer"
      ]
    },
    "children": [
      "flex-layout.col#benefitsLeft",
      "flex-layout.col#benefitsRight"
    ]
  },
  "flex-layout.col#benefitsLeft": {
    "props": {
      "preventVerticalStretch": true,
      "blockClass":["benefitsContent"]
    },
    "children": [
      "flex-layout.row#benefitsLeft-1",
      "flex-layout.row#benefitsLeft-2"
    ]
  },
  "flex-layout.row#benefitsLeft-1": {
    "props": {
      "preventHorizontalStretch": true,
      "preventVerticalStretch": true,
      "blockClass": [
        "benefitsCardContainer"
      ]
    },
    "children": [
      "flex-layout.col#benefitSac",
      "flex-layout.col#benefitDollar",
      "flex-layout.col#benefitTornado"
    ]
  },
  //  SAC
  "flex-layout.col#benefitSac": {
    "props": {
      "preventHorizontalStretch": true,
      "preventVerticalStretch": true,
      "blockClass": "benefitCard"
    },
    "children": [
      "rich-text#mktplaceSac",
      "image#mktplaceSac"
    ]
  },
  "rich-text#mktplaceSac": {
    "props": {
      "text": "**SAC** por nossa conta"
    }
  },
  "image#mktplaceSac": {
    "props": {
      "src": "assets/cassol-marketplace/headphone.png"
    }
  },
  // DOLLAR
  "flex-layout.col#benefitDollar": {
    "props": {
      "preventHorizontalStretch": true,
      "preventVerticalStretch": true,
      "blockClass": "benefitCard"
