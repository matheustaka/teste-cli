{
    "store.custom#outlet-casa-e-decor": {
      "blocks": [
        "list-context.image-list#valentines-main-banner",
        "flex-layout.row#valentines-title-row",
        "flex-layout.row#valentines-shelf-row",
        "list-context.image-list#valentines-banner-slides-2",
        "flex-layout.row#valentines-shelf-row-02"
      ]
    },
  
    "flex-layout.row#valentines-title-row" : {
      "props":{
        "horizontalAlign": "center"
      },
      "children": ["rich-text#valentines-title"]
    },
    "rich-text#valentines-title": {
      "props": {
        "text": "# Outlet Casa & Decoração",
        "textPosition":"center",
        "textAlignment":"center",
        "blockClass": "valentinesTitle"
      }
    },
    "list-context.image-list#valentines-main-banner": {
      "children": [
        "slider-layout#valentines-main-banner"
      ],
      "props": {
        "height": 720,
        "images": [
          {
            "image": "https://cassol.vtexassets.com/assets/vtex.file-manager-graphql/images/aa166a1f-ea11-4106-8df1-569ddf7d0b71___ea24e032afefbec6d51a00dc47c4efd5.png"
          }
        ]
      }
    },
    "slider-layout#valentines-main-banner": {
      "props": {
        "itemsPerPage": {
          "desktop": 1,
          "tablet": 1,
          "phone": 1
        },
        "stopOnHover": true,
        "infinite": true,
        "showNavigationArrows": "desktopOnly",
        "blockClass": "banner-principal"
      }
    },
    "slider-layout#valentines-banner-slides": {
      "props": {
        "itemsPerPage": {
          "desktop": 2,
          "tablet": 2,
          "phone": 1
        },
        "infinite": true,
        "showNavigationArrows": "never",
        "showPaginationDots": "always",
        "centerMode": {
          "desktop": "disabled",
          "mobile": "disabled",
          "phone": "disabled"
        },
        "autoplay" :{
          "timeout": 2800
        },
        "usePagination": true,
        "blockClass": ["valentines-banner-slides"]
      }
    },
  
    "flex-layout.row#valentines-shelf-row" : {
      "props" : {
        "blockClass": ["shelf-block"],
        "colSizing": "auto"
      },
      "children" : ["rich-text#valentine-shelf-title-01","list-context.product-list#valentines-shelf-01"]
    },
  
    "rich-text#valentine-shelf-title-01": {
      "props": {
        "text": "## Vitrine",
        "blockClass" :["valentines-shelf-title"]
      }
    },
    "list-context.product-list#valentines-shelf-01": {
      "blocks": [
        "product-summary.shelf"
      ],
      "children": [
        "slider-layout#valentines-shelf-01"
      ],
      "props": {
        "orderBy": "OrderByBestDiscountDESC",
        "hideUnavailableItems": true
      }
    },
    "slider-layout#valentines-shelf-01": {
      "props": {
        "itemsPerPage": {
          "desktop": 4,
          "tablet": 2,
          "phone": 1
        },
        "showNavigationArrows": "desktopOnly",
        "infinite": true,
        "fullWidth": false,
        "usePagination": true,
        "centerMode": {
          "desktop": "disabled",
          "phone": "to-the-left",
          "mobile": "to-the-left"
        },
        "blockClass": ["shelf", "lp-shelf"]
      },
      "children": []
    },
  
    "flex-layout.row#valentines-shelf-row-02" : {
      "props" : {
        "blockClass": ["shelf-block"],
        "colSizing": "auto"
      },
      "children" : ["rich-text#valentine-shelf-title-02","list-context.product-list#valentines-shelf-02"]
    },
  
    "rich-text#valentine-shelf-title-02": {
      "props": {
        "text": "## Vitrine",
        "blockClass": ["valentines-shelf-title"]
      }
    },
    "list-context.product-list#valentines-shelf-02": {
      "blocks": [
        "product-summary.shelf"
      ],
      "children": [
        "slider-layout#valentines-shelf-02"
      ],
      "props": {
        "orderBy": "OrderByBestDiscountDESC",
        "hideUnavailableItems": true
      }
    },
    "slider-layout#valentines-shelf-02": {
      "props": {
        "itemsPerPage": {
          "desktop": 4,
          "tablet": 2,
          "phone": 1
        },
        "showNavigationArrows": "desktopOnly",
        "infinite": true,
        "fullWidth": false,
        "usePagination": true,
        "centerMode": {
          "desktop": "disabled",
          "phone": "to-the-left",
          "mobile": "to-the-left"
        },
        "blockClass": ["shelf", "lp-shelf"]
      },
      "children": []
    },
    "list-context.image-list#valentines-banner-slides-2": {
      "props": {
        "height" : 400,
        "images": [
          
          {
            "image": "https://cassol.vtexassets.com/assets/vtex.file-manager-graphql/images/4168fcc2-dcd3-42e7-a109-3a1e6be6cfd4___6c0cfdbbf804e51e6cba798abf5e1744.png",
            "width" : "380px"
          },
          {
            "image": "https://cassol.vtexassets.com/assets/vtex.file-manager-graphql/images/e7eea6c3-d01b-4a8e-b531-8616207544e4___ea276b5db69e73612e82eae689319acc.png",
            "width" : "380px"
          },
          {
            "image": "https://cassol.vtexassets.com/assets/vtex.file-manager-graphql/images/d3603fef-4754-49c0-9fe8-11307d2b0bea___df7a7366adec6068102e13fe4315fb00.png",
            "width" : "380px"
          }
        ]
      },
      "children": [
        "slider-layout#valentines-banner-slides-2"
      ]
    },
    "slider-layout#valentines-banner-slides-2": {
      "props": {
        "itemsPerPage": {
          "desktop": 3,
          "tablet": 2,
          "phone": 1
        },
        "infinite": true,
        "showNavigationArrows": "desktopOnly",
        "showPaginationDots": "always",
        "centerMode": {
          "desktop": "disabled",
          "mobile": "to-the-left",
          "phone": "to-the-left"
        },
        "usePagination": true,
        "blockClass": "valentine-banner-slides"
      }
    },
    "slider-layout#valentines-banner-slides-03": {
      "props": {
        "itemsPerPage": {
          "desktop": 2,
          "tablet": 2,
          "phone": 1
        },
        "infinite": true,
        "showNavigationArrows": "never",
        "showPaginationDots": "desktopOnly",
        "centerMode": {
          "desktop": "disabled",
          "mobile": "to-the-left",
          "phone": "to-the-left"
        },
        "usePagination": true
      }
