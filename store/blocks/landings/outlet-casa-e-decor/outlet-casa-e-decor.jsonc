      ],
      "props": {
        "orderBy": "OrderByBestDiscountDESC",
        "hideUnavailableItems": true
      }
    },
    "slider-layout#valentines-shelf-03": {
      "props": {
        "itemsPerPage": {
          "desktop": 4,
          "tablet": 2,
          "phone": 1
        },
        "showNavigationArrows": "desktopOnly",
        "showPaginationDots" : "desktopOnly",
        "infinite": true,
        "fullWidth": true,
