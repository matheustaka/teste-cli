    },
    "children": ["rich-text#shelf-title-03","list-context.product-list#winter-shelf-03"]
  },

  "rich-text#shelf-title-03": {
    "props": {
      "text": "## Vitrine",
      "blockClass": ["winter-shelf-title"]
