    "props": {
      "height" : 400,
      "images": [
        
        {
          "image": "assets/images/bird-380-400.jpg",
          "width" : "380px"
        },
        {
          "image": "assets/images/bird-380-400.jpg",
          "width" : "380px"
        },
        {
          "image": "assets/images/bird-380-400.jpg",
          "width" : "380px"
        }
      ]
    },
    "children": [
      "slider-layout#winter-banner-slides-2"
    ]
  },
  "slider-layout#winter-banner-slides-2": {
    "props": {
      "itemsPerPage": {
        "desktop": 3,
        "tablet": 2,
        "phone": 1
      },
      "infinite": true,
      "showNavigationArrows": "desktopOnly",
      "showPaginationDots": "desktopOnly",
      "centerMode": {
        "desktop": "disabled",
        "mobile": "to-the-left",
        "phone": "to-the-left"
      },
      "usePagination": true
    }
  },
  "list-context.image-list#winter-banner-slides-03": {
    "props": {
      "height": 360,
      "images": [
        {
          "image": "assets/images/whatislove610-360.jpg",
          "width":"610px"
        },
        {
          "image": "assets/images/whatislove610-360.jpg",
          "width":"610px"
        }
      ]
    },
    "children": [
      "slider-layout#winter-banner-slides"
    ]
  },
  "slider-layout#winter-banner-slides-03": {
    "props": {
