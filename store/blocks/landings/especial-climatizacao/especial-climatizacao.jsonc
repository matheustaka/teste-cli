{
  "store.custom#especial-climatizacao": {
    "blocks": [
      "responsive-layout.desktop#desktop",
      "responsive-layout.mobile#mobile"
    ]
  },
  "responsive-layout.desktop#desktop": {
    "children": [
      "list-context.image-list#main",
      "flex-layout.row#mainMiniBanners",
      "flex-layout.row#productShelf",
      "flex-layout.row#mainMiniBanners-2",
      "flex-layout.row#productShelf-2",
      "flex-layout.row#productShelf-3",
      "flex-layout.row#bannersLp"
    ]
  },
  "responsive-layout.mobile#mobile": {
    "children": [
      "list-context.image-list#main",
      "flex-layout.row#mainMiniBanners",
      "flex-layout.row#productShelf",
      "flex-layout.row#mainMiniBanners-2",
      "flex-layout.row#productShelf-2",
      "flex-layout.row#productShelf-3",
      "flex-layout.row#bannersLp"
    ]
  },
  "list-context.image-list#main": {
    "children": [
      "slider-layout#banner-principal"
    ],
    "props": {
      "images": [
        {
          "image": "assets/especial/climatizacao/mainDesktop.jpg",
          "mobileImage": "assets/especial/climatizacao/mainMobile.jpg"
        }
      ]
    }
  },
  "flex-layout.row#mainMiniBanners": {
    "props": {
      "blockClass": [
        "mainContent",
        "especialMiniBanners"
      ]
    },
    "children": [
      "flex-layout.col#image1",
      "flex-layout.col#image2"
    ]
  },
  "flex-layout.col#image1": {
    "props": {
      "marginRight": 5,
      "marginLeft": 5,
      "preventVerticalStretch": true,
      "blockClass":[""]
    },
    "children": [
      "image#mainMini-1"
    ]
  },
  "flex-layout.col#image2": {
    "props": {
      "marginRight": 5,
      "marginLeft": 5,
      "preventVerticalStretch": true
    },
    "children": [
      "image#mainMini-2"
    ]
  },
  "image#mainMini-1": {
    "props": {
      "src": "assets/especial/climatizacao/miniBanner1.jpg"
    }
  },
  "image#mainMini-2": {
    "props": {
      "src": "assets/especial/climatizacao/miniBanner1.jpg"
    }
  },
  "flex-layout.row#productShelf": {
    "props": {
      "blockClass": [
        "mainContent"
      ]
    },
    "children": [
      "flex-layout.col#productShelf"
    ]
  },
  "flex-layout.col#productShelf": {
    "props": {
      "fullWidth": true
    },
    "children": [
      "rich-text#productShelfTitle",
      "list-context.product-list#productShelf"
    ]
  },
  "rich-text#productShelfTitle": {
    "props": {
      "text": "## Acabaram de Chegar",
      "blockClass": "shelfTitle"
    }
  },
  "list-context.product-list#productShelf": {
    "blocks": [
      "product-summary.shelf"
    ],
    "children": [
      "slider-layout#productShelf"
    ],
    "props": {
      "orderBy": "OrderByBestDiscountDESC",
      "hideUnavailableItems": true,
      "collection": "806"
    }
  },
  "slider-layout#productShelf": {
    "props": {
      "itemsPerPage": {
        "desktop": 4,
        "tablet": 3,
        "phone": 2
      },
      "showNavigationArrows": "always",
      "showPaginationDots": "always",
      "infinite": true,
      "fullWidth": true,
      "usePagination": true,
      "blockClass": [
        "especial"
      ]
    }
  },
  "flex-layout.row#mainMiniBanners-2": {
    "props": {
      "blockClass": [
        "mainContent",
        "especialMiniBanners"
      ]
    },
    "children": [
      "flex-layout.col#image3",
      "flex-layout.col#image4"
    ]
  },

  "flex-layout.col#image3": {
    "props": {
      "marginRight": 5,
      "marginLeft": 5,
      "preventVerticalStretch": true
    },
    "children": [
      "image#mainMini-3"
    ]
  },
  "image#mainMini-3": {
    "props": {
      "src": "assets/especial/climatizacao/miniBanner1.jpg"
    }
  },

  "flex-layout.col#image4": {
    "props": {
      "marginRight": 5,
      "marginLeft": 5,
      "preventVerticalStretch": true
    },
    "children": [
      "image#mainMini-4"
    ]
  },
  "image#mainMini-4": {
    "props": {
      "src": "assets/especial/climatizacao/miniBanner1.jpg"
    }
  },
