    ]
  },
  "rich-text#productShelfTitle-3": {
    "props": {
      "text": "## Vitrine de produtos",
      "blockClass": "shelfTitle"
    }
  },
  "list-context.product-list#productShelf-3": {
    "blocks": [
      "product-summary.shelf"
    ],
    "children": [
      "slider-layout#productShelf"
    ],
    "props": {
      "orderBy": "OrderByBestDiscountDESC",
      "hideUnavailableItems": true,
      "collection": "806"
    }
  },
  "flex-layout.row#bannersLp": {
    "props": {
      "blockClass": [
        "mainContent"
      ]
    },
    "children": [
      "flex-layout.col#bannersLp"
    ]
  },
  "flex-layout.col#bannersLp": {
    "props": {
      "blockClass": [
        "mainContent"
      ]
    },
    "children": [
      "image#bannerLp",
      "image#bannerLp-2",
      "image#bannerLp-3"
    ]
  },
  "image#bannerLp": {
    "props": {
      "src": "assets/especial/climatizacao/bannerFinal.jpg"
    }
  },
  "image#bannerLp-2": {
    "props": {
      "src": "assets/especial/climatizacao/bannerFinal.jpg"
    }
  },
  "image#bannerLp-3": {
