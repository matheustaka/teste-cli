{
  "footer": {
    "blocks": ["footer-layout.desktop", "footer-layout.mobile"]
  },
  "footer-layout.desktop": {
    "children": [
      "flex-layout.row#footer-newsletter",
      "flex-layout.row#footer-1-desktop",
      "flex-layout.row#footer-2-desktop",
      "flex-layout.row#footer-3-desktop",
      "flex-layout.row#footer-4-desktop"
    ]
  },

  "flex-layout.row#footer-newsletter" : {
    "children" : ["newsletter-form"],
    "props" : {
      "blockClass" : "newsletterRow"
    }
  },
  
  "newsletter-form": {
    "children": [
      "rich-text#newsletter",
      "newsletter-input-email",
      "newsletter-input-name",
      "newsletter-submit"
    ]
  },

  "rich-text#newsletter": {
    "props": {
      "text": "# Receba as melhores ofertas Cassol!",
      "blockClass" : ["newsTitle"]
    }
  },
  "flex-layout.row#footer-1-desktop": {
    "children": [
      "vtex.menu@2.x:menu#Institucional",
      "vtex.menu@2.x:menu#Politicas",
      "vtex.menu@2.x:menu#Servicos",
      "flex-layout.col#footer-itens-column",
      "flex-layout.col#footer-socialNetworks"
    ],
    "props": {
      "preventVerticalStretch": true,
      "blockClass": "menu-row-footer",
      "paddingTop": 6,
      "paddingBottom": 3
    }
  },

  "flex-layout.col#footer-itens-column": {
    "children": [
      "vtex.menu@2.x:menu#precisa-de-ajuda",
      "vtex.menu@2.x:menu#area-restrita"
