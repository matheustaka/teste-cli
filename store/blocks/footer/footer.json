    "props": {
      "paddingTop": 6,
      "paddingBottom": 3
    }
  },

  "flex-layout.col#footer-socialNetworks": {
    "children": [
      "rich-text#social-networks",
      "flex-layout.row#footer-socialNetworks"
    ],
    "props": {
      "blockClass": "footer-socialNetworks",
      "paddingTop": 6,
      "paddingBottom": 3
    }
  },
  "flex-layout.row#footer-socialNetworks": {
    "children": [
      "rich-text#social-networks-facebook",
      "rich-text#social-networks-instagram",
      "rich-text#social-networks-pinterest",
